{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "baseUrl": "C:/Users/ultis/Documents/Astro/astroDemo",
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/types/*": [
        "./types/*"
      ]
    },
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "./src/**/*",
    "./types/**/*",
    "./services/**/*"
  ]
}